				<jaxb:schemaBindings>
					<jaxb:package name="com.sentenial.origix.ws.directdebit" />
				</jaxb:schemaBindings>
			</jaxb:bindings>			
			
			<jaxb:bindings schemaLocation="paymentschedule/paymentschedule-ws.xsd">
				<jaxb:schemaBindings>
					<jaxb:package name="com.sentenial.origix.ws.paymentschedule" />
				</jaxb:schemaBindings>
			</jaxb:bindings>
			
			<jaxb:bindings schemaLocation="paymentschedule/paymentschedule-ws-wsdl.xsd">
				<jaxb:schemaBindings>
					<jaxb:package name="com.sentenial.origix.ws.paymentschedule" />
				</jaxb:schemaBindings>
			</jaxb:bindings>
			
