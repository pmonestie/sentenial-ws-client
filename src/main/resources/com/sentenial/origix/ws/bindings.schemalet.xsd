				<jaxb:schemaBindings>
					<jaxb:package name="com.sentenial.origix.ws.usermgmnt" />
				</jaxb:schemaBindings>
			</jaxb:bindings>

			<jaxb:bindings schemaLocation="usermgmnt/usermgmnt-ws-wsdl.xsd">
